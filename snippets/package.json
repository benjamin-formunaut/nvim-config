{
  "name": "bmmvim-snippets",
  "_ref": "https://github.com/rafamadriz/friendly-snippets/blob/main/package.json",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "plaintext",
          "markdown",
          "tex",
          "html",
          "global",
          "all"
